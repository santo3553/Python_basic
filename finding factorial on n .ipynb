   "source": [
    "#Find the factorial of an n!"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "b52dd1c7-4ed8-4742-9dca-efc0ec92a425",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The factorial of 5 is: 120\n"
     ]
    }
   ],
   "source": [
    "def factorial_iterative(n):\n",
    "    result = 1\n",
    "    for i in range(1, n + 1):\n",
    "        result *= i\n",
    "    return result\n",
    "\n",
    "n = 5 \n",
    "result = factorial_iterative(n)\n",
    "print(f\"The factorial of {n} is: {result}\")\n"
   ]
  },
